    PanNo = pan_no;
    TanNo = tan_no;
    Remark1 = remark1;
    Remark2 = remark2;
    Remark3 = remark3;
    Gspusername = gspusername;
    Gsppassword = gsppassword;
    Phone = phone;
    Ewbusername = ewbusername;
    Ewbpassword = ewbpassword;
    Businessplace = businessplace;
    Costcenter = costcenter;
